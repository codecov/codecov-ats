{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "ES2021.String"
    ],
    "module": "es2022",
    "moduleResolution": "node",
    "outDir": "dist/",
    "resolveJsonModule": true,
    "rootDir": ".",
    "sourceMap": true,
    "target": "es2017"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts"
  ]
}
